{
  "name": "ledger-wallet-desktop",
  "description": "Ledger Wallet Desktop",
  "repository": "https://github.com/LedgerHQ/ledger-wallet-desktop",
  "version": "0.1.0",
  "author": "Ledger",
  "license": "BSD-2-Clause",
  "scripts": {
    "compile": "electron-webpack",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "dist": "bash ./scripts/dist.sh && electron-builder",
    "flow": "flow",
    "lint": "eslint src webpack .storybook",
    "postinstall": "bash ./scripts/postinstall.sh",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"{src,webpack,.storybook}/**/*.js\"",
    "publish-storybook": "bash ./scripts/publish-storybook.sh",
    "release": "build",
    "start": "bash ./scripts/start.sh",
    "storybook": "start-storybook -p 4444"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "./webpack/renderer.config.js"
    },
    "main": {
      "webpackConfig": "./webpack/main.config.js"
    }
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
